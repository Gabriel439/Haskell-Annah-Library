let End (f : * -> * -> *) (a : *) : #Path f a a =
        \(Path : forall (a : *) -> forall (c : *) -> *)
    ->  \(  Step
        :   forall (a : *)
        ->  forall (b : *)
        ->  forall (c : *)
        ->  forall (head : f a b)
        ->  forall (tail : Path b c)
        ->  Path a c
        )
    ->  \(End : forall (a : *) -> Path a a)
    ->  End a
in  End
