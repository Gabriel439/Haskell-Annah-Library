let first (a : *) : #Monoid (#Maybe a) =
    let c (m1 : #Maybe a) (m2 : #Maybe a) : #Maybe a =
        \(Maybe   : *                          )
    ->  \(Nothing : Maybe                      )
    ->  \(Just    : forall (_Just : a) -> Maybe)
    ->  m1
        Maybe
        (m2 Maybe Nothing Just)
        Just
    in  \(l : (#List (#Maybe a))) -> l (#Maybe a) c (#Maybe/Nothing a)
in  first
