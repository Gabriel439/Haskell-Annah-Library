let Eq (a : *) (b : *) (eqA : #Eq a) (eqB : #Eq b) : #Eq (#Either a b) =
        \(e1 : #Either a b)
    ->  \(e2 : #Either a b)
    ->  e1
        #Bool
        (\(l1 : a) ->
            e2
            #Bool
            (\(l2 : a) -> eqA l1 l2)
            (\(_  : b) -> #Bool/False )
        )
        (\(r1 : b) ->
            e2
            #Bool
            (\(_  : a) -> #Bool/False )
            (\(r2 : b) -> eqB r1 r2)
        )
in  Eq
