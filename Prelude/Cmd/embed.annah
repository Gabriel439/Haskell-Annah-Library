let embed
    (f : * -> *)
    (g : * -> *)
    (k : forall (a : *) -> f a -> #Cmd g a)
    (a : *)
    (x : #Cmd f a)
    : #Cmd g a
    =   \(Cmd : *)
    ->  \(Bind : forall (b : *) -> g b -> (b -> Cmd) -> Cmd)
    ->  \(Pure : a -> Cmd)
    ->  x
        Cmd
        (\(b : *) -> \(m : f b) -> k b m Cmd Bind)
        Pure
in  embed
