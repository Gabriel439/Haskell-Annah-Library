let show
    (a : *)
    (showType : #String )
    (showA : a -> #String )
    (xs : #List a) : #String =
    #String/concat
    [nil #String
    ,   "[nil "
    ,   showType
    ,   #String/concat
        (#List/map a #String (\(x : a) -> #String/(++) ", " (showA x)) xs)
    ,   "]"
    ]
in  show
