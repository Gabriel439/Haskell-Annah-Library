let show (bin : #Bin ) : #String =
    let str : #String = bin
            #String
            "0"
            (\(bin_ : #Bin/Bin_ ) ->
                #String/(++)
                "1"
                (   bin_
                    #String
                    ""
                    (#String/(++) "0")
                    (#String/(++) "1")
                )
            )
    in #String/(++) str "b"
in  show
