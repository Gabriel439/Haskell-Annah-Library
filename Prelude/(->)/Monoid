λ(e : *) → λ(a : *) → λ(monoid : ../Monoid  a) → λ(fs : ../List  (../(->)  e a)) → ./Monad  e a (λ(Cmd : *) → λ(Bind : ∀(b : *) → ../(->)  e b → (b → Cmd) → Cmd) → λ(Pure : ∀(x : a) → Cmd) → Bind (../List  a) (./Monad  e (../List  a) (../Monad/sequence  (../(->)  e) a fs)) (λ(xs : ../List  a) → Bind a (./pure  e a (monoid xs)) Pure))
